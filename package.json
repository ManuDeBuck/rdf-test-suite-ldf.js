{
  "name": "rdf-test-suite-ldf",
  "version": "1.0.0",
  "description": "An engine testing suite",
  "keywords": [
    "sparql",
    "ldf",
    "linked data",
    "query engine",
    "test suite"
  ],
  "main": "index.js",
  "typings": "index",
  "repository": "https://github.com/ManuDeBuck/rdf-test-suite-ldf.js.git",
  "author": "Manu De Buck <manu.debuck@telenet.be>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ManuDeBuck/rdf-test-suite-ldf.js/issues"
  },
  "homepage": "https://github.com/ManuDeBuck/rdf-test-suite-ldf.js#readme",
  "contributors": [],
  "bin": {
    "rdf-test-suite-ldf": "bin/Runner.js"
  },
  "files": [
    "bin/**/*.d.ts",
    "bin/**/*.js",
    "lib/**/*.d.ts",
    "lib/**/*.js",
    "lib/**/*.json",
    "index.d.ts",
    "index.js"
  ],
  "pre-commit": [
    "build",
    "lint",
    "test"
  ],
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfig": "test/tsconfig.json"
      }
    },
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "testRegex": "(/test/.*|(\\.|/)(test|spec))\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "collectCoverage": true,
    "testEnvironment": "node"
  },
  "scripts": {
    "test": "jest ${1}",
    "test-watch": "jest ${1} --watch",
    "coveralls": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "lint": "tslint index.ts lib/**/*.ts test/**/*.ts --exclude '**/*.d.ts'",
    "build": "tsc",
    "validate": "npm ls",
    "prepare": "npm run build",
    "version": "manual-git-changelog onversion"
  },
  "devDependencies": {
    "@comunica/actor-init-sparql-file": "^1.9.1",
    "@comunica/actor-init-sparql-hdt": "^1.5.0",
    "@types/jest": "^24.0.17",
    "@types/minimist": "^1.2.0",
    "@types/n3": "^1.1.0",
    "coveralls": "^3.0.6",
    "istanbul": "^0.4.5",
    "jest": "^24.8.0",
    "jest-rdf": "^1.3.0",
    "nock": "^10.0.6",
    "pre-commit": "^1.2.2",
    "precommit": "^1.2.2",
    "ts-jest": "^24.0.2",
    "tslint": "^5.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "@comunica/mediatortype-time": "^1.9.0",
    "global": "^4.4.0",
    "minimist": "^1.2.0",
    "n3": "^1.1.1",
    "rdf-test-suite": "^1.9.3"
  }
}
